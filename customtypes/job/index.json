{
  "id": "job",
  "label": "Job",
  "format": "custom",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "company": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": ""
        }
      },
      "uid": {
        "config": {
          "label": "UID"
        },
        "type": "UID"
      },
      "project_id": {
        "type": "Text",
        "config": {
          "label": "Project ID",
          "placeholder": ""
        }
      },
      "start_date": {
        "type": "Text",
        "config": {
          "label": "Start Date",
          "placeholder": ""
        }
      },
      "end_date": {
        "type": "Text",
        "config": {
          "label": "End Date",
          "placeholder": ""
        }
      },
      "position": {
        "type": "Text",
        "config": {
          "label": "Position",
          "placeholder": ""
        }
      },
      "description": {
        "type": "StructuredText",
        "config": {
          "label": "Description",
          "placeholder": "",
          "allowTargetBlank": false,
          "single": "paragraph,strong,em"
        }
      },
      "responsibilities": {
        "type": "StructuredText",
        "config": {
          "label": "Responsibilities",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "list-item,o-list-item,paragraph,strong,em,hyperlink"
        }
      },
      "duties": {
        "type": "StructuredText",
        "config": {
          "label": "Duties",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "paragraph,hyperlink,strong,em,list-item,o-list-item"
        }
      },
      "tech_stack": {
        "type": "Group",
        "config": {
          "label": "Tech Stack",
          "fields": {
            "tech": {
              "type": "Link",
              "config": {
                "label": "tech",
                "select": "document",
                "customtypes": ["skill"]
              }
            }
          }
        }
      }
    }
  }
}
